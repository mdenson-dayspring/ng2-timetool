
{
  "manifest_version": 2,
  "name": "Dayspring Timetool Chrome Extension",
  "description": "This extension is eventually going to get information from the timesheet.",
  "version": "1.0",
  "icons": {
    "128": "128.png"
  },
  "browser_action": {
    "default_icon": "assets/img/icon.png",
    "default_popup": "index.html",
    "default_title": "Today"
  },
  "content_security_policy": "script-src 'self' https://www.google.com 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "activeTab",
    "http://malachi/",
    "https://www.google.com/jsapi"
  ]
}
